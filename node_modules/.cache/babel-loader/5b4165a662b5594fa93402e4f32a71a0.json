{"ast":null,"code":"import { useStore } from 'vuex';\nimport BButton from '@/components/BButton.vue';\nimport { computed } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomeView\",\n  components: {\n    BButton: BButton\n  },\n  setup: function setup() {\n    var store = useStore();\n    axios.get('/list').then(function (list) {\n      store.commit('setUplist', list);\n    });\n\n    var logout = function logout() {\n      store.commit('setUserData', {});\n      store.commit('setUserlogin', false);\n      alert(\"로그아웃\");\n    };\n\n    return {\n      logout: logout,\n      userData: computed(function () {\n        return store.state.userData;\n      }),\n      userlogin: computed(function () {\n        return store.state.userlogin;\n      })\n    };\n  }\n};","map":{"version":3,"mappings":"AAYA,SAASA,QAAT,QAAyB,MAAzB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,SAASC,QAAT,QAAyB,KAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE;IAAEJ,OAAM,EAANA;EAAF,CAFC;EAGbK,KAHa,mBAGL;IACN,IAAMC,KAAI,GAAIP,QAAQ,EAAtB;IACEG,KAAK,CAACK,GAAN,CAAU,OAAV,EAAmBC,IAAnB,CAAwB,UAACC,IAAD,EAAQ;MAC9BH,KAAK,CAACI,MAAN,CAAa,WAAb,EAA0BD,IAA1B;IACD,CAFD;;IAKA,IAAME,MAAK,GAAI,SAATA,MAAS,GAAM;MACnBL,KAAK,CAACI,MAAN,CAAa,aAAb,EAA4B,EAA5B;MACAJ,KAAK,CAACI,MAAN,CAAa,cAAb,EAA6B,KAA7B;MAEAE,KAAK,CAAC,MAAD,CAAL;IACF,CALA;;IAMA,OAAO;MACLD,MAAM,EAANA,MADK;MAELE,QAAQ,EAAEZ,QAAQ,CAAC;QAAA,OAAMK,KAAK,CAACQ,KAAN,CAAYD,QAAlB;MAAA,CAAD,CAFb;MAGLE,SAAS,EAAEd,QAAQ,CAAC;QAAA,OAAMK,KAAK,CAACQ,KAAN,CAAYC,SAAlB;MAAA,CAAD;IAHd,CAAP;EAKH;AArBY,CAAf","names":["useStore","BButton","computed","axios","name","components","setup","store","get","then","list","commit","logout","alert","userData","state","userlogin"],"sourceRoot":"","sources":["/Users/se-uggim/Desktop/vue-crud-board/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <router-link to=\"/signup\" v-if=\"!userData.id\">회윈가입</router-link>\n    <b-button v-if='userlogin' @click=\"logout\">로그아웃</b-button>\n    <router-link v-if=\"userData.id\" to=\"/write\">글작성</router-link>\n    <router-link v-else to=\"/login\">로그인</router-link>\n    <p v-if=\"userData.id\">{{userData.name}}환영합니다</p>\n    \n  </div>\n</template>\n\n<script>\nimport { useStore } from 'vuex';\nimport BButton from '@/components/BButton.vue';\nimport { computed } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: \"HomeView\",\n  components: { BButton },\n  setup() {\n    const store = useStore();\n      axios.get('/list').then((list)=>{\n        store.commit('setUplist', list);\n      });\n\n\n      const logout = () => {\n        store.commit('setUserData', {});\n        store.commit('setUserlogin', false);\n        \n        alert(\"로그아웃\")\n      }\n      return {\n        logout,\n        userData: computed(() => store.state.userData),\n        userlogin: computed(() => store.state.userlogin),\n      };\n  },\n}\n</script>\n\n"]},"metadata":{},"sourceType":"module"}