{"ast":null,"code":"import { useStore } from 'vuex';\nimport BButton from '@/components/BButton.vue';\nimport { computed } from 'vue';\nimport Listup from '@/components/Listup.vue';\nexport default {\n  name: \"HomeView\",\n  components: {\n    BButton: BButton\n  },\n  setup: function setup() {\n    var store = useStore();\n\n    var logout = function logout() {\n      localStorage.removeItem('user');\n      store.commit('setUserData', {});\n      alert(\"로그아웃\");\n    };\n\n    var local = function local() {\n      if (localStorage.getItem('user')) {\n        return true;\n      }\n    };\n\n    local();\n    console.log(local);\n    return {\n      logout: logout,\n      userData: computed(function () {\n        return store.state.userData;\n      }),\n      local: local\n    };\n  }\n};","map":{"version":3,"mappings":"AAYA,SAASA,QAAT,QAAyB,MAAzB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,SAASC,QAAT,QAAyB,KAAzB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE;IAAEJ,OAAM,EAANA;EAAF,CAFC;EAGbK,KAHa,mBAGL;IACJ,IAAMC,KAAI,GAAIP,QAAQ,EAAtB;;IACA,IAAMQ,MAAK,GAAI,SAATA,MAAS,GAAM;MACnBC,YAAY,CAACC,UAAb,CAAwB,MAAxB;MACAH,KAAK,CAACI,MAAN,CAAa,aAAb,EAA4B,EAA5B;MACAC,KAAK,CAAC,MAAD,CAAL;IACF,CAJA;;IAKA,IAAMC,KAAI,GAAI,SAARA,KAAQ,GAAM;MAClB,IAAGJ,YAAY,CAACK,OAAb,CAAqB,MAArB,CAAH,EAAgC;QAC9B,OAAO,IAAP;MACF;IACF,CAJA;;IAKAD,KAAK;IACLE,OAAO,CAACC,GAAR,CAAYH,KAAZ;IACA,OAAO;MACLL,MAAM,EAANA,MADK;MAELS,QAAQ,EAAEf,QAAQ,CAAC;QAAA,OAAMK,KAAK,CAACW,KAAN,CAAYD,QAAlB;MAAA,CAAD,CAFb;MAGLJ,KAAK,EAALA;IAHK,CAAP;EAKH;AAtBY,CAAf","names":["useStore","BButton","computed","Listup","name","components","setup","store","logout","localStorage","removeItem","commit","alert","local","getItem","console","log","userData","state"],"sourceRoot":"","sources":["/Users/se-uggim/Desktop/vue-crud-board/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <router-link to=\"/signup\" v-if=\"!userData.id\">회윈가입</router-link>\n    <b-button @click=\"logout\">로그아웃</b-button>\n    <router-link v-if=\"userData.id\" to=\"/write\">글작성</router-link>\n    <router-link v-else to=\"/login\">로그인</router-link>\n    <p v-if=\"userData.id\">{{userData.name}}환영합니다</p>\n    \n  </div>\n</template>\n\n<script>\nimport { useStore } from 'vuex';\nimport BButton from '@/components/BButton.vue';\nimport { computed } from 'vue';\nimport Listup from '@/components/Listup.vue'\n\nexport default {\n  name: \"HomeView\",\n  components: { BButton },\n  setup() {\n      const store = useStore();\n      const logout = () => {\n        localStorage.removeItem('user');\n        store.commit('setUserData', {});\n        alert(\"로그아웃\")\n      }\n      const local = () => {\n        if(localStorage.getItem('user')){\n          return true;\n        }\n      }\n      local();\n      console.log(local);\n      return {\n        logout,\n        userData: computed(() => store.state.userData),\n        local,\n      };\n  },\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}